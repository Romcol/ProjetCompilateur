PROGRAMME iter1
VAR ENTIER s,i,j,n;



	i=1;
	j=2;

	SI j>i ALORS
		ECRIRE("OK");

		SI j<i ALORS
			ECRIRE("POK");
		SINON
			ECRIRE("OK");

			SI j<i ALORS
				ECRIRE("POK");
			SINON
				ECRIRE("OK");
			FSI ;

		FSI ;

	SINON
		ECRIRE("POK");
	FSI ;
FPROGRAMME

	i=1; j=1; n=10;

	TANTQUE i<=n FAIRE

		j=1;
		TANTQUE j<=n FAIRE
			ECRIRE("(");
			ECRIRE(i);
			ECRIRE(",");
			ECRIRE(j);
			ECRIRE(")");
			ALALIGNE;
			j=j+1;
		FAIT;

		i=i+1;
	FAIT;